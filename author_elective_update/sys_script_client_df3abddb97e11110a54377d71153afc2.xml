        <name>NeedIt Set Requested For Mgr</name>
        <order/>
        <script><![CDATA[function onChange(control, oldValue, newValue, isLoading, isTemplate) {

    if (isLoading && !g_form.isNewRecord() || newValue === '' || newValue == oldValue) {
        return;
    }
    var ga = new GlideAjax('ClassGetManager');

    ga.addParam('sysparm_Mgrname', 'getManagerName');
    ga.addParam('sysparam_requestedFor', g_form.getValue('u_requested_for'));

    ga.getXML(populateManagerName);

    function populateManagerName(response) {
        var answer = response.responseXML.documentElement.getAttribute("answer");
        g_form.addInfoMessage("answer " + answer);

        //g_form.setValue('u_manager', answer);
    }
}]]></script>
